{
		"name": "wikimedia/takedown-tools",
		"description": "Tool for processing legal takedowns",
    "type": "project",
    "license": "MIT",
    "require": {
				"php": "^7.1.3",
				"doctrine/doctrine-bundle": "^1.6",
				"doctrine/orm": "^2.5",
				"geosocio/entity-attacher-bundle": "^1.0.4",
				"geosocio/serialize-response": "^1.0.2",
				"guzzlehttp/guzzle": "^6.2",
				"lexik/jwt-authentication-bundle": "^2.4",
				"mediawiki/oauthclient": "^0.1.2",
				"nixilla/php-jwt": "^0.1.1",
				"sensio/framework-extra-bundle": "^3.0",
				"symfony/cache": "^3.3",
				"symfony/expression-language": "^3.3",
				"symfony/flex": "^1.0",
				"symfony/framework-bundle": "^3.3",
				"symfony/property-info": "^3.3",
				"symfony/serializer": "^3.3",
				"symfony/yaml": "^3.3"
		},
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "mediawiki/mediawiki-codesniffer": "^0.8.0",
        "symfony/dotenv": "^3.3",
        "symfony/var-dumper": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %WEB_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BJH7BVMYPGR0JP12TF8VT30J",
            "allow-contrib": "true"
        }
    }
}
